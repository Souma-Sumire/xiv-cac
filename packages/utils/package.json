{
  "name": "xiv-cac-utils",
  "version": "0.0.4",
  "author": "InfSein",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InfSein/xiv-cac.git",
    "directory": "packages/utils"
  },
  "description": "Provide utils for xiv-cac standard.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}
